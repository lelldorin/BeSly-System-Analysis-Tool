	fi
	if (split(msg$, part$(), ":|") > 3) then
		PartFour$ = part$(4)
	fi
	if (msg$ <> "") print msg$
	
	switch msg$
	
	case AppInternalName$+":App:Quit|"
	case AppInternalName$+":_QuitRequested|"
		WINDOW CLOSE AppInternalName$
		break
		
	//User Info	
	
	case "BT_X|"
